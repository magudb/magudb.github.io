---
layout: null
---
[
  {% for post in site.posts %}
    {
      "title": "{{ post.title | xml_escape }}",     
      "url": " {{ post.url | xml_escape }}",    
      "content": "{{ post.content | strip_html | strip_newlines | replace_regex: '\W', ' ' | xml_escape }}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
]
